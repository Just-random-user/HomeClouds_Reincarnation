
@{
    ViewData["Title"] = "File explorer";
}
<div class="container">
    <div class="row row-cols-auto">
        @foreach (var path in (List<string>)ViewData["Paths"])
        {
            <div class="col">
                @await Component.InvokeAsync("File", new Clouds.ViewModels.FileViewModel(path))
            </div>
        }
    </div>
</div>
